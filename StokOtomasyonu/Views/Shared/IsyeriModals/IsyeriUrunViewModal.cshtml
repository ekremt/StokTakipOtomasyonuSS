

<div class="modal fade" id="IsyeriUrunViewModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form>
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">İşyeri Ürünleri</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table table-striped mt-4">

                        <thead>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">Ürün Adı</th>
                                <th scope="col">Ürün Model</th>
                                <th scope="col">Ürün Seri No</th>
                                <th scope="col">Ürün Tarihi</th>
                                <th scope="col">Ürün Fiyatı</th>
                            </tr>
                        </thead>
                        <tbody>

                            <tr ng-repeat="item in urunInIsyeri ">
                                <th scope="row">{{$index + 1}}</th>
                                <td>{{ item.urun_adi }}</td>
                                <td>{{ item.urun_model }}</td>
                                <td>{{ item.urun_seri_no }}</td>
                                <td>{{ item.urun_tarihi.split('/')[1].split('(')[1].split(')')[0] | date:"dd.MM.y HH:mm:ss"  }} </td>
                                <td>{{ item.urun_fiyati }}</td>

                                


                            </tr>
                             </tbody>
                    </table>


                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">Kapat</button>
                   
                </div>
            </form>
        </div>

    </div>
</div>


